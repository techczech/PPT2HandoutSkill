{
  "name": "pptx-handout-site",
  "version": "1.0.0",
  "description": "Interactive handout website generated from PowerPoint presentations",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/processMedia.js && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "extract": "python scripts/extract-pptx.py"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "tailwindcss": "^4.1.10",
    "typescript": "~5.8.3",
    "vite": "^7.3.1"
  }
}
